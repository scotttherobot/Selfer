<h4> Log In </h4>
<%= form_tag method:"post" do %>
   <p>
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %>
   </p>
   <p>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
   </p>
   <p class="button"><%= submit_tag "log in", :class => "btn btn-success" %></p>
<% end %>
